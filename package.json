{
  "name": "tamagui-monorepo",
  "private": true,
  "version": "0.0.0",
  "repository": {
    "url": "https://github.com/tamagui/tamagui"
  },
  "workspaces": [
    "./code/compiler/**/*",
    "./code/core/**/*",
    "./code/demos",
    "./code/kitchen-sink",
    "./code/tests/**/*",
    "./code/sandbox",
    "./code/packages/**/*",
    "./code/tamagui.dev",
    "./code/ui/**/*"
  ],
  "resolutions": {
    "@swc/core": "1.14.0",
    "@react-navigation/core": "7.9.2",
    "@react-navigation/native": "7.1.9",
    "@react-navigation/native-stack": "7.6.2",
    "@react-navigation/routers": "7.4.0",
    "@react-navigation/elements": "2.8.1",
    "react": "19.1.0",
    "react-dom": "19.1.0"
  },
  "overrides": {
    "react": "19.1.0",
    "react-dom": "19.1.0"
  },
  "scripts": {
    "build:app": "./scripts/ci-build-app.sh",
    "build:force": "bun run clean:build && bun run build -- --force",
    "build:js": "bun run build --force -- --skip-types",
    "build:pre": "cd code/packages/babel-plugin-fully-specified && bun run build",
    "build:types": "SKIP_JS=1 bun run build",
    "build:verbose": "bun run build -vvv",
    "build": "bun run turbo:run build",
    "check:deps": "manypkg check",
    "check:unused": "knip --dependencies --workspace .",
    "check:tamagui": "./node_modules/.bin/tamagui check",
    "check": "bun run check:deps && bun run check:unused && bun run check:tamagui && bun run check:references",
    "ci": "bun install && bun run check && bun run lint && bun run typecheck && bun run build && bun run test",
    "clean:build": "./scripts/run.sh clean:build",
    "clean:ts": "tsc -b --clean tsconfig.build.json",
    "clean": "./scripts/run.sh clean:build",
    "create": "./node_modules/.bin/create-tamagui",
    "dev:build": "bun run --filter @tamagui/dot-dev build:prod",
    "dev:extract": "TAMAGUI_EXTRACT=1 bun run --filter @tamagui/dot-dev dev:clean",
    "dev:prod-only-homepage": "bun run dev:build --only index && bun run dev:serve",
    "dev:prod": "bun run dev:build && bun run dev:serve",
    "dev:serve": "bun run --filter @tamagui/dot-dev serve",
    "dev": "bun run --filter @tamagui/dot-dev dev:clean",
    "docker:build-and-run": "bun run docker:build && bun run docker:run",
    "docker:build": "dotenv -e .env -- sh -c 'docker build --progress=plain -t my-app .'",
    "docker:run": "dotenv -e .env -- sh -c 'docker run -it -p 3000:3000 my-app'",
    "docker:serve": "./scripts/ci-serve.sh",
    "docs:build": "bun run --filter @tamagui/one-docs build:prod",
    "docs": "bun run --filter @tamagui/one-docs dev",
    "fix:deps": "manypkg fix",
    "fix": "bun run lint:fix",
    "kitchen-sink:android": "bun run --filter @tamagui/kitchen-sink start:android",
    "kitchen-sink:build:android": "bun run --filter @tamagui/kitchen-sink android",
    "kitchen-sink:build:ios": "bun run --filter @tamagui/kitchen-sink ios",
    "kitchen-sink:clean": "bun run --filter @tamagui/kitchen-sink start:clean",
    "kitchen-sink:extract": "bun run --filter @tamagui/kitchen-sink start:extract",
    "kitchen-sink:prod": "bun run --filter @tamagui/kitchen-sink start:prod",
    "kitchen-sink:web": "bun run --filter @tamagui/kitchen-sink start:web",
    "kitchen-sink": "bun run --filter @tamagui/kitchen-sink start",
    "lighthouse:live": "npx lighthouse https://tamagui.dev --view --chrome-flags='--headless' --throttling.cpuSlowdownMultiplier=16 --output-path=/tmp/report.html",
    "lighthouse:prod": "npx lighthouse http://localhost:3333 --view --chrome-flags='--headless' --throttling.cpuSlowdownMultiplier=16 --output-path=/tmp/report.html",
    "lighthouse:vite:prod": "npx lighthouse http://localhost:8080 --view --chrome-flags='--headless' --throttling.cpuSlowdownMultiplier=16 --output-path=/tmp/report.html",
    "lighthouse": "npx lighthouse http://localhost:3000 --view --chrome-flags='--headless' --throttling.cpuSlowdownMultiplier=16 --output-path=/tmp/report.html",
    "lint:fix": "ultra -r --no-pretty lint:fix",
    "lint": "ultra -r --no-pretty lint",
    "llink": "lllink ~/one",
    "next-site": "bun run --filter @tamagui/next-site dev",
    "profile": "tsx ./scripts/bun-profile.ts",
    "release:canary": "bun run release --canary --ci",
    "release": "tsx ./scripts/release.ts",
    "check:references": "bun ./scripts/check-references.ts",
    "check:references:fix": "bun ./scripts/check-references.ts --fix",
    "repair": "tsx ./scripts/repair.ts",
    "reset-package-json": "RESET_PACKAGE_JSON=true node -e \"require('./scripts/multi-react-version-tests.js').resetPackageJson()\"",
    "sandbox": "bun run --filter sandbox dev",
    "scripts:format": "tsx ./scripts/format.ts",
    "scripts:setup": "tsx ./scripts/setup.ts",
    "starter:native": "bun run --filter @tamagui/starter native",
    "starter:prod": "bun run --filter @tamagui/starter web:prod && bun run --filter @tamagui/starter web:prod:serve",
    "starter": "bun run --filter @tamagui/starter web",
    "studio:tauri": "bun run --filter @tamagui/studio dev:tauri",
    "studio": "bun run --filter @tamagui/studio dev",
    "takeout:build": "bun run --filter @tamagui/takeout build:prod",
    "takeout:prod": "bun run takeout:build && bun run takeout:serve",
    "takeout:serve": "bun run --filter @tamagui/takeout serve",
    "takeout": "bun run --filter @tamagui/takeout dev",
    "test-ios": "turbo run test-ios --concurrency=1",
    "test:native:detox": "cd code/kitchen-sink && bun run ../packages/native-ci/src/cli.ts test ios --project-root .",
    "test:ci": "turbo test --concurrency=1",
    "test:demos": "bun run --filter @tamagui/demos test",
    "test:docker:build": "dotenv -e .env -- sh -c 'docker build -t tamagui-test -f test/Dockerfile.test .'",
    "test:docker:run": "docker run --rm tamagui-test",
    "test:docker": "bun run test:docker:build && bun run test:docker:run",
    "test": "CLEANUP=1 bun run turbo:run --concurrency=1 test",
    "test:web": "bun run turbo:run test:web --concurrency=1",
    "turbo:run": "TURBO_TELEMETRY_DISABLED=1 turbo",
    "typecheck:perf": "bun run --filter @tamagui/monorepo-test test",
    "typecheck": "./scripts/typecheck.sh",
    "up-dep": "tko update-deps",
    "upgrade:one:canary": "bun run up-dep 'one'@canary '@vxrn/*'@canary",
    "upgrade:one": "bun run up-dep 'one'@latest '@vxrn/*'@latest",
    "upgrade:starters:expo-router": "cd code/starters/expo-router && bun install && bun run upgrade:tamagui",
    "upgrade:starters:free": "cd ../starter-free && bun install && bun run upgrade:tamagui",
    "upgrade:starters:remix": "cd code/starters/remix && bun install && bun run upgrade:tamagui",
    "upgrade:starters:simple-web": "cd code/starters/simple-web && bun install && bun run upgrade:tamagui",
    "upgrade:starters": "bun run upgrade:starters:simple-web && bun run upgrade:starters:expo-router && bun run upgrade:starters:remix && bun run upgrade:starters:free",
    "vercel:build": "bun run build:js && cd code/old-site && bun run ci:build",
    "watch:js": "SKIP_TYPES=1 bun run watch:packages",
    "watch:packages": "SKIP_INITIAL_BUILD=1 SKIP_TYPES_INITIAL=1 ./scripts/run.sh watch",
    "watch:ts": "bun ./scripts/watch-ts.ts",
    "watch": "npm-run-all --parallel watch:ts watch:packages"
  },
  "manypkg": {
    "workspaceProtocol": "require"
  },
  "profile": {
    "//": "bun run profile [name] to toggle package resolutions",
    "react-18": {
      "applied": false,
      "resolutions": {
        "react": "18.3.1",
        "react-dom": "18.3.1"
      }
    }
  },
  "packageManager": "bun@1.2.22",
  "trustedDependencies": [
    "@swc/core",
    "sharp"
  ],
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@manypkg/cli": "^0.23.0",
    "@playwright/test": "^1.49.1",
    "@take-out/cli": "^0.0.40",
    "@testing-library/dom": "^10.4.0",
    "@typescript/native-preview": "^7.0.0-dev.20260119.1",
    "dotenv-cli": "^7.4.2",
    "fs-extra": "^11.2.0",
    "knip": "^5.70.2",
    "lllink": "^1.1.430",
    "npm-run-all": "^4.1.5",
    "p-map": "^7.0.3",
    "prompts": "2.1.0",
    "react": ">=19",
    "react-native-screens": "~4.16.0",
    "tsx": "^4.20.6",
    "turbo": "^2.1.3",
    "typescript": "~5.9.2",
    "ultra-runner": "^3.10.5"
  }
}
