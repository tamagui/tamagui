{
  "name": "tamagui-monorepo",
  "private": true,
  "version": "0.0.0",
  "repository": {
    "url": "https://github.com/tamagui/tamagui"
  },
  "workspaces": [
    "./code/compiler/**/*",
    "./code/core/**/*",
    "./code/demos",
    "./code/kitchen-sink",
    "./code/tests/**/*",
    "./code/sandbox",
    "./code/packages/**/*",
    "./code/tamagui.dev",
    "./code/ui/**/*"
  ],
  "resolutions": {
    "@swc/core": "1.14.0",
    "@react-navigation/core": "7.9.2",
    "@react-navigation/native": "7.1.9",
    "@react-navigation/native-stack": "7.6.2",
    "@react-navigation/routers": "7.4.0",
    "@react-navigation/elements": "2.8.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "overrides": {
    "@swc/core": "1.14.0",
    "@react-navigation/core": "7.9.2",
    "@react-navigation/native": "7.1.9",
    "@react-navigation/native-stack": "7.6.2",
    "@react-navigation/routers": "7.4.0",
    "@react-navigation/elements": "2.8.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "scripts": {
    "build:app": "./scripts/ci-build-app.sh",
    "build:force": "bun run clean:build && bun run build -- --force",
    "build:js": "bun run build --force -- --skip-types",
    "build:pre": "cd code/packages/babel-plugin-fully-specified && bun run build",
    "build:types": "SKIP_JS=1 bun run build",
    "build:verbose": "bun run build -vvv",
    "build": "turbo run build",
    "check:deps": "manypkg check",
    "check:unused": "knip --dependencies --workspace .",
    "check:tamagui": "./node_modules/.bin/tamagui check",
    "check": "bun run check:deps && bun run check:unused && bun run check:tamagui && bun run check:references",
    "ci": "bun install && bun run check && bun run lint && bun run typecheck && bun run build && bun run test",
    "clean:build": "./scripts/run.sh clean:build",
    "clean:ts": "tsc -b --clean tsconfig.build.json",
    "clean": "./scripts/run.sh clean:build",
    "create": "./node_modules/.bin/create-tamagui",
    "dev:build": "bun --filter @tamagui/dot-dev run build:prod",
    "dev:extract": "TAMAGUI_EXTRACT=1 bun --filter @tamagui/dot-dev run dev:clean",
    "dev:prod-only-homepage": "bun run dev:build --only index && bun run dev:serve",
    "dev:prod": "bun run dev:build && bun run dev:serve",
    "dev:serve": "bun --filter @tamagui/dot-dev run serve",
    "dev": "bun --filter @tamagui/dot-dev run dev:clean",
    "docker:build-and-run": "bun run docker:build && bun run docker:run",
    "docker:build": "dotenv -e .env -- sh -c 'docker build --progress=plain -t my-app .'",
    "docker:run": "dotenv -e .env -- sh -c 'docker run -it -p 3000:3000 my-app'",
    "docker:serve": "./scripts/ci-serve.sh",
    "docs:build": "bun --filter @tamagui/one-docs run build:prod",
    "docs": "bun --filter @tamagui/one-docs run dev",
    "fix:deps": "bun run manypkg fix",
    "fix": "bun run lint:fix",
    "generate-css": "bun --filter @tamagui/themes run generate && bun --filter @tamagui/themes run build && bun --filter @tamagui/dot-dev run build",
    "kitchen-sink:android": "bun --filter @tamagui/kitchen-sink run start:android",
    "kitchen-sink:build:android": "bun --filter @tamagui/kitchen-sink run android",
    "kitchen-sink:build:ios": "bun --filter @tamagui/kitchen-sink run ios",
    "kitchen-sink:clean": "bun --filter @tamagui/kitchen-sink run start:clean",
    "kitchen-sink:extract": "bun --filter @tamagui/kitchen-sink run start:extract",
    "kitchen-sink:prod": "bun --filter @tamagui/kitchen-sink run start:prod",
    "kitchen-sink:web": "bun --filter @tamagui/kitchen-sink run start:web",
    "kitchen-sink": "bun --filter @tamagui/kitchen-sink run start",
    "lighthouse:live": "npx lighthouse https://tamagui.dev --view --chrome-flags='--headless' --throttling.cpuSlowdownMultiplier=16 --output-path=/tmp/report.html",
    "lighthouse:prod": "npx lighthouse http://localhost:3333 --view --chrome-flags='--headless' --throttling.cpuSlowdownMultiplier=16 --output-path=/tmp/report.html",
    "lighthouse:vite:prod": "npx lighthouse http://localhost:8080 --view --chrome-flags='--headless' --throttling.cpuSlowdownMultiplier=16 --output-path=/tmp/report.html",
    "lighthouse": "npx lighthouse http://localhost:3000 --view --chrome-flags='--headless' --throttling.cpuSlowdownMultiplier=16 --output-path=/tmp/report.html",
    "lint:fix": "ultra -r --no-pretty lint:fix",
    "lint": "ultra -r --no-pretty lint",
    "llink": "lllink ~/one",
    "next-site": "bun --filter @tamagui/next-site run dev",
    "release:canary": "bun release --canary --ci",
    "release": "bun ./scripts/release.ts",
    "check:references": "node -r esbuild-register ./scripts/check-references.ts",
    "check:references:fix": "node -r esbuild-register ./scripts/check-references.ts --fix",
    "repair": "node -r esbuild-register ./scripts/repair.ts",
    "reset-package-json": "RESET_PACKAGE_JSON=true node -e \"require('./scripts/multi-react-version-tests.js').resetPackageJson()\"",
    "sandbox": "bun --filter sandbox run dev",
    "scripts:format": "node -r esbuild-register ./scripts/format.ts",
    "scripts:setup": "node -r esbuild-register ./scripts/setup.ts",
    "starter:native": "bun --filter @tamagui/starter run native",
    "starter:prod": "bun --filter @tamagui/starter run web:prod && bun --filter @tamagui/starter run web:prod:serve",
    "starter": "bun --filter @tamagui/starter run web",
    "studio:tauri": "bun --filter @tamagui/studio run dev:tauri",
    "studio": "bun --filter @tamagui/studio run dev",
    "takeout:build": "bun --filter @tamagui/takeout run build:prod",
    "takeout:prod": "bun run takeout:build && bun run takeout:serve",
    "takeout:serve": "bun --filter @tamagui/takeout run serve",
    "takeout": "bun --filter @tamagui/takeout run dev",
    "test-ios": "bun workspaces foreach --exclude tamagui -A run test-ios",
    "test:ci": "turbo test --concurrency=1",
    "test:demos": "bun --filter @tamagui/demos run test",
    "test:docker:build": "dotenv -e .env -- sh -c 'docker build -t tamagui-test -f test/Dockerfile.test .'",
    "test:docker:run": "docker run --rm tamagui-test",
    "test:docker": "bun run test:docker:build && bun run test:docker:run",
    "test": "CLEANUP=1 turbo run --concurrency=1 test",
    "test:web": "turbo run test:web --concurrency=1",
    "typecheck:perf": "bun --filter @tamagui/monorepo-test run test",
    "typecheck": "./scripts/typecheck.sh",
    "upgrade:one:canary": "bun up 'one'@canary '@vxrn/*'@canary",
    "upgrade:one": "bun up 'one'@latest '@vxrn/*'@latest",
    "upgrade:starters:expo-router": "cd code/starters/expo-router && bun install && bun run upgrade:tamagui",
    "upgrade:starters": "bun run upgrade:starters:expo-router",
    "vercel:build": "bun run build:js && cd code/old-site && bun run ci:build",
    "watch:js": "SKIP_TYPES=1 bun run watch:packages",
    "watch:packages": "SKIP_INITIAL_BUILD=1 SKIP_TYPES_INITIAL=1 ./scripts/run.sh watch",
    "watch:ts": "bun ./scripts/watch-ts.ts",
    "watch": "npm-run-all --parallel watch:ts watch:packages"
  },
  "manypkg": {
    "workspaceProtocol": "require",
    "ignoredRules": [
      "INVALID_DEV_AND_PEER_DEPENDENCY_RELATIONSHIP"
    ]
  },
  "packageManager": "bun@1.2.22",
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@manypkg/cli": "^0.23.0",
    "@playwright/test": "^1.49.1",
    "@testing-library/dom": "^10.4.0",
    "dotenv-cli": "^7.4.2",
    "esbuild": "^0.25.11",
    "esbuild-register": "^3.6.0",
    "fs-extra": "^11.2.0",
    "knip": "^5.70.2",
    "lllink": "^1.1.430",
    "npm-run-all": "^4.1.5",
    "p-map": "^7.0.3",
    "prompts": "2.1.0",
    "react-native-screens": "~4.16.0",
    "turbo": "^2.1.3",
    "typescript": "~5.9.2",
    "ultra-runner": "^3.10.5"
  }
}
