{
  "name": "@tamagui/react-native-web-lite",
  "version": "1.139.2",
  "type": "module",
  "description": "React Native for Web",
  "sideEffects": false,
  "main": "dist/cjs",
  "module": "dist/esm",
  "files": [
    "types",
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tamagui-build --skip-types --skip-native",
    "watch": "tamagui-build --watch --skip-types --skip-native",
    "clean": "tamagui-build clean",
    "clean:build": "tamagui-build clean:build"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": "./dist/esm/index.mjs",
      "require": "./dist/cjs/index.cjs",
      "default": "./dist/cjs/index.native.js"
    },
    "./without-animated": {
      "import": "./dist/esm/without-animated.mjs",
      "require": "./dist/cjs/without-animated.cjs"
    },
    "./Libraries/Utilities/codegenNativeComponent": {
      "import": "./dist/esm/codegenNativeComponent.mjs",
      "require": "./dist/cjs/codegenNativeComponent.cjs"
    },
    "./Libraries/Utilities/codegenNativeCommand": {
      "import": "./dist/esm/codegenNativeCommand.mjs",
      "require": "./dist/cjs/codegenNativeCommand.cjs"
    },
    "./dist/exports/StyleSheet/compiler/createReactDOMStyle": {
      "import": "./dist/esm/createReactDOMStyle.mjs",
      "require": "./dist/cjs/createReactDOMStyle.cjs"
    },
    "./dist/modules/*": {
      "import": "./dist/esm/modules/*/index.mjs",
      "require": "./dist/cjs/modules/*/index.cjs"
    },
    "./dist/exports/*": {
      "import": "./dist/esm/exports/*/index.mjs",
      "require": "./dist/cjs/exports/*/index.cjs"
    },
    "./dist/*": {
      "import": "./dist/esm/*/index.mjs",
      "require": "./dist/cjs/*/index.cjs"
    }
  },
  "dependencies": {
    "@tamagui/normalize-css-color": "workspace:*",
    "@tamagui/react-native-use-pressable": "workspace:*",
    "@tamagui/react-native-use-responder-events": "workspace:*",
    "@tamagui/react-native-web-internals": "workspace:*",
    "@tamagui/web": "workspace:*",
    "invariant": "^2.2.4",
    "memoize-one": "^6.0.0"
  },
  "devDependencies": {
    "@tamagui/build": "workspace:*",
    "react": "*",
    "react-dom": "*"
  },
  "peerDependencies": {
    "react": "*",
    "react-dom": "*"
  },
  "license": "MIT",
  "tags": [
    "react"
  ],
  "keywords": [
    "react",
    "react-component",
    "react-native",
    "web"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/tamagui/tamagui.git",
    "directory": "packages/react-native-web-lite"
  },
  "bugs": {
    "url": "https://github.com/tamagui/tamagui/issues"
  }
}
