{
  "name": "@tamagui/native-ci",
  "version": "2.0.0-rc.14",
  "description": "Native CI/CD helpers for React Native apps with Expo - fingerprinting, caching, and build optimization",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/tamagui/tamagui.git",
    "directory": "code/packages/native-ci"
  },
  "bin": "./dist/cli.mjs",
  "source": "src/index.ts",
  "files": [
    "src",
    "types",
    "dist",
    "actions",
    "action.yml"
  ],
  "type": "module",
  "module": "dist",
  "types": "./types/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./types/index.d.ts",
      "browser": "./dist/index.mjs",
      "module": "./dist/index.mjs",
      "import": "./dist/index.mjs"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tamagui-build --skip-native",
    "watch": "bun run build --watch",
    "clean": "tamagui-build clean",
    "clean:build": "tamagui-build clean:build"
  },
  "dependencies": {
    "@expo/fingerprint": "^0.15.3"
  },
  "devDependencies": {
    "@tamagui/build": "workspace:*",
    "@types/bun": "^1.1.0",
    "@types/node": "^22.1.0"
  },
  "engines": {
    "node": ">=18"
  }
}
