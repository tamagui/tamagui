{
  "name": "example-skunk",
  "version": "1.1.317",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "$npm_execpath db:check && $npm_execpath one dev",
    "dev:clean": "one dev --clean",
    "clean": "one clean",
    "prebuild:native": "one prebuild",
    "build:web": "one build",
    "serve": "one serve",
    "ios": "EXPO_NO_TELEMETRY=true expo run:ios",
    "android": "EXPO_NO_TELEMETRY=true expo run:android",
    "upgrade:tamagui": "$npm_execpath up '*tamagui*' '@tamagui/*'",
    "db:init": "$npm_execpath db:generate && $npm_execpath db:push && $npm_execpath db:seed",
    "db:check": "bash -c \"$npm_execpath db:push 2>&1 >/dev/null | grep ECONNREFUSED >/dev/null; if [ $? -eq 1 ]; then echo '\n\n‚ùå Note! Error running db:push, you may need to set up and run your db. See README.md\n\n'; exit 1; fi\"",
    "db:generate": "drizzle-kit generate --config ./code/db/drizzle.config.ts",
    "db:push": "drizzle-kit push --config ./code/db/drizzle.config.ts",
    "db:migrate": "dotenvx run -f .env -- tsx ./code/db/run-migrations.ts",
    "db:seed": "dotenvx run -f .env -- tsx ./code/db/seed.ts"
  },
  "dependencies": {
    "@dominicstop/ts-event-emitter": "^1.1.0",
    "@dotenvx/dotenvx": "^1.12.1",
    "@tamagui/config": "1.115.5",
    "@tamagui/image-next": "1.115.5",
    "@tamagui/lucide-icons": "1.115.5",
    "@vxrn/color-scheme": "^1.1.317",
    "dotenv": "^16.4.5",
    "drizzle-kit": "^0.24.2",
    "drizzle-orm": "^0.33.0",
    "expo": "^51.0.0",
    "expo-modules-core": "^1.12.24",
    "one": "1.1.317",
    "postgres": "^3.4.4",
    "react": "^18.3.1",
    "react-native": "0.74.1",
    "react-native-ios-modal": "^3.0.0-5",
    "react-native-ios-utilities": "^5.0.0-45",
    "react-native-reanimated": "~3.11.0",
    "react-native-safe-area-context": "^4.11.0",
    "react-native-screens": "3.31.1",
    "react-native-svg": "15.7.1",
    "react-native-web": "^0.19.12",
    "tamagui": "1.115.5"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.2",
    "@tamagui/vite-plugin": "1.115.5",
    "tsx": "^4.19.0",
    "vite": "6.0.0-beta.1"
  }
}
