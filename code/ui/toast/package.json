{
  "name": "@tamagui/toast",
  "version": "2.0.0-rc.13",
  "source": "src/index.ts",
  "files": [
    "src",
    "types",
    "dist"
  ],
  "type": "module",
  "sideEffects": [
    "*.css"
  ],
  "main": "dist/cjs",
  "module": "dist/esm",
  "types": "./types/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./types/index.d.ts",
      "react-native": "./dist/esm/index.native.js",
      "module": "./dist/esm/index.mjs",
      "import": "./dist/esm/index.mjs",
      "require": "./dist/cjs/index.cjs",
      "default": "./dist/cjs/index.native.js"
    },
    "./v1": {
      "react-native": {
        "module": "./dist/esm/v1.native.js",
        "import": "./dist/esm/v1.native.js",
        "require": "./dist/cjs/v1.native.js"
      },
      "types": "./types/v1.d.ts",
      "module": "./dist/esm/v1.mjs",
      "import": "./dist/esm/v1.mjs",
      "require": "./dist/cjs/v1.cjs"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tamagui-build",
    "watch": "tamagui-build --watch",
    "clean": "tamagui-build clean",
    "clean:build": "tamagui-build clean:build"
  },
  "dependencies": {
    "@tamagui/animate-presence": "workspace:*",
    "@tamagui/collection": "workspace:*",
    "@tamagui/compose-refs": "workspace:*",
    "@tamagui/constants": "workspace:*",
    "@tamagui/core": "workspace:*",
    "@tamagui/create-context": "workspace:*",
    "@tamagui/dismissable": "workspace:*",
    "@tamagui/helpers": "workspace:*",
    "@tamagui/native": "workspace:*",
    "@tamagui/polyfill-dev": "workspace:*",
    "@tamagui/portal": "workspace:*",
    "@tamagui/stacks": "workspace:*",
    "@tamagui/start-transition": "workspace:*",
    "@tamagui/text": "workspace:*",
    "@tamagui/use-controllable-state": "workspace:*",
    "@tamagui/visually-hidden": "workspace:*"
  },
  "devDependencies": {
    "@tamagui/build": "workspace:*",
    "react": ">=19",
    "react-native": "0.81.5"
  },
  "peerDependencies": {
    "react": ">=19",
    "react-native": "*"
  },
  "module:jsx": "dist/jsx"
}
