{
  "name": "@tamagui/portal",
  "version": "2.0.0-1768983999155",
  "type": "module",
  "sideEffects": [
    "*.css"
  ],
  "source": "src/index.ts",
  "types": "./types/index.d.ts",
  "main": "dist/cjs",
  "module": "dist/esm",
  "files": [
    "src",
    "types",
    "dist"
  ],
  "scripts": {
    "build": "tamagui-build",
    "watch": "tamagui-build --watch",
    "lint": "biome check src",
    "lint:fix": "biome check --write src",
    "clean": "tamagui-build clean",
    "clean:build": "tamagui-build clean:build"
  },
  "typesVersions": {
    "*": {
      "setup-native": [
        "./types/setup-native.d.ts"
      ],
      "native-portal": [
        "./types/native-portal.d.ts"
      ]
    }
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "react-native": {
        "module": "./dist/esm/index.native.js",
        "import": "./dist/esm/index.native.js",
        "require": "./dist/cjs/index.native.js"
      },
      "types": "./types/index.d.ts",
      "module": "./dist/esm/index.mjs",
      "import": "./dist/esm/index.mjs",
      "require": "./dist/cjs/index.cjs",
      "default": "./dist/cjs/index.native.js"
    },
    "./setup-native": {
      "react-native": {
        "types": "./types/setup-native.d.ts",
        "module": "./dist/esm/setup-native.js",
        "import": "./dist/esm/setup-native.js",
        "require": "./dist/cjs/setup-native.js"
      },
      "types": "./types/setup-native.d.ts",
      "module": "./dist/esm/setup-native.mjs",
      "import": "./dist/esm/setup-native.mjs",
      "require": "./dist/cjs/setup-native.cjs"
    }
  },
  "dependencies": {
    "@tamagui/constants": "workspace:*",
    "@tamagui/core": "workspace:*",
    "@tamagui/native-portal": "workspace:*",
    "@tamagui/start-transition": "workspace:*",
    "@tamagui/use-event": "workspace:*",
    "@tamagui/web": "workspace:*",
    "@tamagui/z-index-stack": "workspace:*"
  },
  "devDependencies": {
    "@tamagui/build": "workspace:*",
    "react": "*",
    "react-dom": "*",
    "react-native": "0.81.5"
  },
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "react": "*",
    "react-dom": "*",
    "react-native": "*"
  }
}
