{
  "name": "@tamagui/focus-guard",
  "version": "2.0.0-rc.0",
  "source": "src/index.ts",
  "files": [
    "src",
    "types",
    "dist"
  ],
  "sideEffects": true,
  "main": "dist/cjs",
  "module": "dist/esm",
  "types": "./types/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tamagui-build",
    "watch": "tamagui-build --watch",
    "lint": "../../node_modules/.bin/biome check src",
    "lint:fix": "../../node_modules/.bin/biome check --write --unsafe src",
    "clean": "tamagui-build clean",
    "clean:build": "tamagui-build clean:build"
  },
  "dependencies": {
    "@tamagui/compose-refs": "workspace:*",
    "@tamagui/use-event": "workspace:*"
  },
  "devDependencies": {
    "@tamagui/build": "workspace:*",
    "react": ">=19"
  },
  "peerDependencies": {
    "react": ">=19"
  },
  "module:jsx": "dist/jsx"
}
