    "@tamagui/get-button-sized": "1.112.8",
    "@tamagui/helpers": "1.112.8",
    "@tamagui/helpers-tamagui": "1.112.8",
    "@tamagui/stacks": "1.112.8",
    "@tamagui/text": "1.112.8",
    "@tamagui/web": "1.112.8"
  },
  "devDependencies": {
    "@tamagui/build": "1.112.8",
    "react": "^18.2.0 || ^19.0.0",
    "vitest": "2.1.1"
  },
  "publishConfig": {
    "access": "public"
