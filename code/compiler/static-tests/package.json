{
  "name": "@tamagui/static-tests",
  "version": "2.0.0-rc.0-1769885482630",
  "private": true,
  "license": "MIT",
  "module": "dist/esm/index.js",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "pretest": "RUN_PRETEST=1 node -r esbuild-register ./tests/lib/preTest.js",
    "test-output": "node -r esbuild-register ./test-output.tsx",
    "test": "npm run test:native && npm run test:web",
    "test:update": "npm run test:extract:web -- --update && npm run test:babel:native -- --update && npm run test:babel:web -- --update && npm run test:styled:web -- --update && npm run test:flatten:native -- --update && npm run test:webpack -- --update",
    "test:run": "INCLUDE_CSS_COLOR_NAMES=1 vitest --retry=2 --config ../../packages/vite-plugin-internal/src/vite.config.cjs.ts --run --dangerouslyIgnoreUnhandledErrors",
    "test:run:web": "DISABLE_NATIVE_TEST=1 npm run test:run --",
    "test:concatClassName": "npm run test:run -- tests/concatClassName.web.test.tsx",
    "test:webpack": "npm run pretest && npm run test:run -- tests/webpack.test.tsx --reporter=verbose --pool=forks",
    "test:flatten:native": "npm run test:run -- tests/flatten.native.test.tsx",
    "test:babel:native": "npm run test:run -- tests/babel.native.test.tsx",
    "test:babel:web": "npm run test:run -- tests/babel.web.test.tsx",
    "test:extract:web": "npm run test:run:web -- tests/extract.web.test.tsx",
    "test:styled:web": "npm run test:run -- tests/styled.web.test.tsx",
    "test:web": "npm run test:run:web -- tests/*.web.test.tsx && npm run test:webpack",
    "test:native": "npm run test:run -- tests/*.native.test.tsx"
  },
  "devDependencies": {
    "@babel/plugin-syntax-typescript": "^7.25.4",
    "@babel/types": "^7.25.4",
    "@tamagui/sandbox-ui": "workspace:*",
    "@tamagui/test-design-system": "workspace:*",
    "@testing-library/react": "^16.1.0",
    "@types/babel__generator": "^7.6.4",
    "@types/babel__traverse": "^7.18.2",
    "@types/find-root": "^1.1.2",
    "@types/node": "^22.1.0",
    "@types/webpack": "^4.41.26",
    "babel-loader": "^9.1.3",
    "css-loader": "^6.7.4",
    "dedent": "^0.7.0",
    "esbuild-loader": "4.3.0",
    "null-loader": "^4.0.1",
    "react": ">=19",
    "react-native-web": "^0.21.0",
    "react-test-renderer": "^19.0.0",
    "style-loader": "^3.3.3",
    "typescript": "~5.9.2",
    "vitest": "4.0.4",
    "webpack": "5.99.7"
  },
  "peerDependencies": {
    "react": ">=19",
    "react-native-web": "*"
  },
  "tests": {
    "parallel": true
  }
}
