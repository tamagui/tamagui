{
  "name": "@tamagui/next-plugin",
  "version": "1.135.6",
  "type": "module",
  "source": "src/index.ts",
  "license": "MIT",
  "types": "./types/index.d.ts",
  "main": "dist/cjs",
  "module": "dist/esm",
  "files": [
    "src",
    "types",
    "dist"
  ],
  "scripts": {
    "build": "tamagui-build --skip-native",
    "watch": "tamagui-build --skip-native --watch",
    "lint": "biome check src",
    "lint:fix": "biome check --write src",
    "clean": "tamagui-build clean",
    "clean:build": "tamagui-build clean:build"
  },
  "exports": {
    "./package.json": "./package.json",
    "./reset.css": "./reset.css",
    ".": {
      "types": "./types/index.d.ts",
      "import": "./dist/esm/index.mjs",
      "require": "./dist/cjs/index.cjs",
      "default": "./dist/cjs/index.native.js"
    }
  },
  "dependencies": {
    "@babel/preset-react": "^7.24.7",
    "@tamagui/proxy-worm": "workspace:*",
    "@tamagui/react-native-svg": "workspace:*",
    "@tamagui/static": "workspace:*",
    "babel-loader": "^9.1.3",
    "browserslist": "^4.22.2",
    "css-loader": "^6.7.4",
    "esbuild-loader": "^4.2.2",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "tamagui-loader": "workspace:*",
    "url-loader": "^4.1.1"
  },
  "devDependencies": {
    "@tamagui/build": "workspace:*",
    "next": "^15.3.1",
    "webpack": "^5.88.2"
  },
  "peerDependencies": {
    "next": ">=13"
  },
  "publishConfig": {
    "access": "public"
  },
  "gitHead": "a49cc7ea6b93ba384e77a4880ae48ac4a5635c14"
}
