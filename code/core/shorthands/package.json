{
  "name": "@tamagui/shorthands",
  "version": "2.0.0-rc.12-1771191583546",
  "gitHead": "a49cc7ea6b93ba384e77a4880ae48ac4a5635c14",
  "source": "src/index.ts",
  "files": [
    "src",
    "types",
    "dist",
    "*.cjs",
    "v4",
    "v5"
  ],
  "type": "module",
  "sideEffects": false,
  "main": "dist/cjs",
  "module": "dist/esm",
  "types": "./types/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./types/index.d.ts",
      "react-native": "./dist/esm/index.native.js",
      "module": "./dist/esm/index.mjs",
      "import": "./dist/esm/index.mjs",
      "require": "./dist/cjs/index.cjs",
      "default": "./dist/cjs/index.native.js"
    },
    "./v4": {
      "types": "./types/v4.d.ts",
      "react-native": "./dist/esm/v4.native.js",
      "module": "./dist/esm/v4.mjs",
      "import": "./dist/esm/v4.mjs",
      "require": "./dist/cjs/v4.cjs"
    },
    "./v5": {
      "types": "./types/v5.d.ts",
      "react-native": "./dist/esm/v5.native.js",
      "module": "./dist/esm/v5.mjs",
      "import": "./dist/esm/v5.mjs",
      "require": "./dist/cjs/v5.cjs"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tamagui-build",
    "watch": "tamagui-build --watch",
    "clean": "tamagui-build clean",
    "clean:build": "tamagui-build clean:build"
  },
  "dependencies": {
    "@tamagui/web": "workspace:*"
  },
  "devDependencies": {
    "@tamagui/build": "workspace:*"
  }
}
