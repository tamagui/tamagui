{
  "name": "@tamagui/native-portal",
  "version": "2.0.0-1769035103261",
  "type": "module",
  "sideEffects": false,
  "types": "./types/index.d.ts",
  "main": "dist/cjs",
  "module": "dist/esm",
  "files": [
    "src",
    "types",
    "dist"
  ],
  "scripts": {
    "build": "tamagui-build",
    "watch": "tamagui-build --watch",
    "lint": "biome check src",
    "lint:fix": "biome check --write src",
    "clean": "tamagui-build clean",
    "clean:build": "tamagui-build clean:build"
  },
  "typesVersions": {
    "*": {
      "setup": [
        "./types/setup.d.ts"
      ]
    }
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./types/index.d.ts",
      "module": "./dist/esm/index.mjs",
      "import": "./dist/esm/index.mjs",
      "require": "./dist/cjs/index.cjs"
    },
    "./setup": {
      "react-native": {
        "types": "./types/setup.d.ts",
        "module": "./dist/esm/setup.mjs",
        "import": "./dist/esm/setup.mjs",
        "require": "./dist/cjs/setup.cjs"
      },
      "types": "./types/setup.d.ts",
      "module": "./dist/esm/setup.mjs",
      "import": "./dist/esm/setup.mjs",
      "require": "./dist/cjs/setup.cjs"
    }
  },
  "devDependencies": {
    "@tamagui/build": "workspace:*",
    "react": "*",
    "react-native": "0.81.5",
    "react-native-teleport": "^0.5.6"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*",
    "react-native-teleport": ">=0.5.0"
  },
  "peerDependenciesMeta": {
    "react-native-teleport": {
      "optional": true
    }
  },
  "publishConfig": {
    "access": "public"
  }
}
