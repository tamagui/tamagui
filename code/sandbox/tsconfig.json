{
  "extends": "../../tsconfig",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "~/*": [
        "./*"
      ],
      // tsconfig doesnt support merging so we have to duplicate:
      "tamagui": [
        "../ui/tamagui"
      ],
      "@tamagui/*": [
        "../core/*",
        "../ui/*",
        "../packages/*"
      ],
      "@tamagui/demos": [
        "../demos"
      ]
    },
    "types": [
      "vite/client"
    ],
    "noEmit": true
  }
}
