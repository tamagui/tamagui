{
  "name": "@tamagui/remix-starter",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "packageManager": "yarn@4.5.0",
  "scripts": {
    "build": "remix vite:build",
    "dev": "remix vite:dev",
    "start": "remix-serve ./build/server/index.js",
    "typecheck": "tsc",
    "prod": "npm run build && npm run start",
    "lint": "biome check ./app",
    "check:tamagui": "tamagui check",
    "lint:fix": "biome check ./app --fix",
    "upgrade:tamagui": "yarn up '*tamagui*'@latest '@tamagui/*'@latest"
  },
  "dependencies": {
    "@remix-run/node": "^2.12.1",
    "@remix-run/react": "^2.12.1",
    "@remix-run/serve": "^2.12.1",
    "@tamagui/config": "^1.126.5",
    "@tamagui/core": "^1.126.5",
    "@tamagui/vite-plugin": "^1.126.5",
    "isbot": "^4.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tamagui": "^1.126.5"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.2",
    "@remix-run/dev": "^2.12.1",
    "@tamagui/cli": "^1.126.5",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.38.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.4",
    "typescript": "^5.1.6",
    "vite": "^5.1.0",
    "vite-tsconfig-paths": "^4.2.1"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
