{
  "name": "@tamagui/kitchen-sink",
  "version": "1.134.0",
  "private": true,
  "main": "index.js",
  "scripts": {
    "start:one": "one dev",
    "start:web": "DISABLE_EXTRACTION=true NODE_ENV=development webpack serve",
    "start:web:prod": "NODE_ENV=production webpack serve",
    "start:web:extract": "DISABLE_EXTRACTION=false NODE_ENV=development webpack serve",
    "start": "EXPO_NO_TELEMETRY=true expo start --dev-client --offline",
    "start:extract": "TAMAGUI_OPTIMIZE_NATIVE_VIEWS=1 TAMAGUI_ENABLE_DYNAMIC_LOAD=1 yarn start",
    "start:prod": "DISABLE_EXTRACTION=false TAMAGUI_ENABLE_DYNAMIC_LOAD=1 expo start --dev-client --offline --no-dev --minify",
    "start:tamagui": "tama dev",
    "start:android": "yarn start --android",
    "start:ios": "yarn start --ios",
    "start:clean": "watchman watch-del-all & rm -r $TMPDIR/metro-cache & yarn start -c",
    "pod": "./pod-install.sh",
    "android": "EXPO_NO_TELEMETRY=true yarn expo run:android",
    "ios": "yarn pod && EXPO_NO_TELEMETRY=true yarn expo run:ios",
    "ios:prod": "yarn ios --configuration Release",
    "eject": "EXPO_NO_TELEMETRY=true expo eject",
    "screenshot": "node -r esbuild-register ../../node_modules/.bin/playwright test --config ./playwright-screenshot.config.ts",
    "test:native": "maestro test ./flows --exclude-tags=util",
    "test:web": "yarn test:web:driver-rn && yarn test:web:driver-css && yarn test:web:driver-moti",
    "test:web:driver-css": "echo 'testing css\\n' && NODE_ENV=test TAMAGUI_TEST_ANIMATION_DRIVER=css node -r esbuild-register ../../node_modules/.bin/playwright test",
    "test:web:driver-rn": "echo 'testing rn\\n' && NODE_ENV=test TAMAGUI_TEST_ANIMATION_DRIVER=native node -r esbuild-register ../../node_modules/.bin/playwright test",
    "test:web:driver-moti": "echo 'testing moti\\n' && NODE_ENV=test TAMAGUI_TEST_ANIMATION_DRIVER=moti node -r esbuild-register ../../node_modules/.bin/playwright test",
    "test:web:debug": "yarn test:web --debug",
    "test-ios": "vitest --config ./vitest.config.ios.mts --run",
    "prod:web": "NODE_ENV=production webpack --json=dist/compilation-stats.json",
    "test": "yarn test:web"
  },
  "dependencies": {
    "@dominicstop/ts-event-emitter": "1.1.0",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-navigation/native": "^7.0.14",
    "@react-navigation/native-stack": "^7.2.0",
    "@shopify/restyle": "^2.4.4",
    "@tamagui/animations-css": "workspace:*",
    "@tamagui/animations-moti": "workspace:*",
    "@tamagui/animations-motion": "workspace:*",
    "@tamagui/animations-react-native": "workspace:*",
    "@tamagui/bento": "workspace:*",
    "@tamagui/cli": "workspace:*",
    "@tamagui/colors": "workspace:*",
    "@tamagui/config": "workspace:*",
    "@tamagui/core": "workspace:*",
    "@tamagui/demos": "workspace:*",
    "@tamagui/font-inter": "workspace:*",
    "@tamagui/get-token": "workspace:*",
    "@tamagui/lucide-icons": "workspace:*",
    "@tamagui/sandbox-ui": "workspace:*",
    "@tamagui/sheet": "workspace:*",
    "@tamagui/shorthands": "workspace:*",
    "@tamagui/themes": "workspace:*",
    "@tamagui/toast": "workspace:*",
    "@tamagui/web": "workspace:*",
    "burnt": "^0.12.2",
    "expo": "54.0.10",
    "expo-constants": "~18.0.9",
    "expo-document-picker": "~14.0.7",
    "expo-font": "~14.0.8",
    "expo-image-picker": "~17.0.8",
    "expo-linear-gradient": "~15.0.7",
    "expo-linking": "~8.0.8",
    "expo-splash-screen": "~31.0.10",
    "react": "*",
    "react-dom": "*",
    "react-native": "^0.81.4",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-reanimated": "^4.1.0",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-web": "^0.21.0",
    "solito": "^4.3.0",
    "tamagui": "workspace:*",
    "vitest": "^3.2.4",
    "webdriverio": "^9.5.4"
  },
  "packageManager": "yarn@4.5.0",
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@playwright/test": "^1.49.1",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.10",
    "@tamagui/babel-plugin": "workspace:*",
    "babel-plugin-module-resolver": "^5.0.2",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^2.9.1",
    "one": "1.1.515",
    "pod-install": "^0.3.2",
    "tilg": "0.1.1",
    "webpack": "^5.88.2",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.8.1"
  }
}
