{
  "$schema": "https://turbo.build/schema.json",
  "extends": ["//"],
  "pipeline": {
    "build:js": {
      "cache": false
    },
    "build": {
      "dependsOn": ["^build"],
      "inputs": [
        "tsconfig.json",
        "package.json",
        "src/**",
        "tamagui-build.js",
        "externalNodePlugin.js"
      ],
      "outputs": [".next/**", "dist/**", "types/**"]
    },
    "start": {
      "dependsOn": ["^build"],
      "outputs": []
    },
    "test": {
      "dependsOn": ["^build:js"],
      "outputs": []
    },
    "clean": {
      "outputs": ["dist/**"],
      "cache": false
    },
    "clean:build": {
      "outputs": [],
      "cache": false
    },
    "lint": {
      "outputs": []
    },
    "lint:fix": {
      "outputs": ["src/**"]
    },
    "dev": {
      "cache": false
    }
  }
}
